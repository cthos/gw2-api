{
  "name": "@cthos/gw2-api",
  "version": "2.2.2",
  "description": "GuildWars 2 API interface",
  "main": "lib/index.js",
  "module": "lib-es6/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cthos/gw2-api.git"
  },
  "scripts": {
    "build": "rm -rf lib && tsc && webpack && npm run build-es6",
    "build-es6": "rm -rf lib-es6 && tsc --esModuleInterop --outDir lib-es6 -m es6 -t es5",
    "test": "mocha test --timeout=40000",
    "docs": "jsdoc index.js src/ -d docs/ -R README.md -r"
  },
  "dependencies": {
    "chunk": "0.0.2",
    "md5": "^2.2.1",
    "object.assign": "^4.0.3",
    "promise": "^7.0.4",
    "request": "^2.88.0",
    "underscore": "^1.8.3"
  },
  "keywords": [
    "gw2",
    "api"
  ],
  "author": "cthos",
  "license": "ISC",
  "devDependencies": {
    "mocha": "^6.1.4",
    "ts-loader": "^5.4.4",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}
